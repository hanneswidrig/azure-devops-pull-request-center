{
  "name": "azure-pull-request-hub",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@uifabric/utilities": "^7.5.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.0-next.39",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package-release": "npm run clean-build && tfx extension create --root ./ --output-path ./azure-devops-pull-request-center.vsix",
    "package-dev": "npm run clean-build && tfx extension create --root ./ --output-path ./azure-devops-pull-request-center-develop.vsix --manifest-globs ./vss-extension.json --overrides-file ./vss-extension.dev.json --share-with hannes-widrig",
    "clean-build": "del-cli build/**/runtime~main*.map build/**/runtime~main*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "azure-devops-extension-api": "^1.157.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^1.160.2",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.0",
    "del-cli": "^3.0.0",
    "office-ui-fabric-react": "^7.59.2",
    "react-router-dom": "^5.1.2",
    "rename-webpack-plugin": "^2.0.0",
    "tfx-cli": "^0.7.9",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  }
}
